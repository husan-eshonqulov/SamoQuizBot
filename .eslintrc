{
  "parser": "@typescript-eslint/parser",
  "extends": [
    "airbnb-base",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "plugins": ["@typescript-eslint", "prettier"],
  "env": {
    "node": true,
    "es6": true
  },
  "rules": {
    "prettier/prettier": "error",
    "eslint-plugin-import/extensions": "always"
  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
}
